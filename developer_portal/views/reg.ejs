<% include header.ejs %>
<form class="form-horizontal" method="post">
<fieldset>
<legend>account register</legend>
<div class="control-group">
<label class="control-label" for="username">account</label>
<div class="controls">
<input type="text" class="input-xlarge" id="username" name="account">
<p class="help-block">your account for developer portal</p>
</div>
</div>
<div class="control-group">
<label class="control-label" for="password">password</label>
<div class="controls">
<input type="password" class="input-xlarge" id="password" name="password">
</div>
</div>
<div class="control-group">
<label class="control-label" for="password-repeat">password-repeat</label>
<div class="controls">
<input type="password" class="input-xlarge" id="password-repeat"
name="password-repeat">
</div>
</div>
<div class="form-actions">
<button type="submit" class="btn btn-primary">sign up</button>
</div>
</fieldset>
</form>
